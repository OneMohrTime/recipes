  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OneMohrTime/recipes/issues"
  },
  "browserslist": [
    "IE 9-11",
    "last 20 versions"
  ],
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-connect-php": "^1.0.3",
    "gulp-sass": "^4.1.0",
    "gulp-uglifycss": "^1.1.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "bourbon": "^6.0.0",
    "clipboard": "^2.0.7",
    "cross-env": "^7.0.0",
    "include-media": "^1.4.9",
    "laravel-mix": "^5.0.1",
    "locomotive-scroll": "^4.1.0",
    "normalize.css": "^8.0.1",
    "svg4everybody": "^2.1.9",
    "tippy.js": "^6.3.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
